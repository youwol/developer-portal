            "@youwol/rx-tree-views": "^0.3.0",
            "@youwol/rx-tab-views": "^0.3.0",
            "@youwol/os-top-banner": "^0.2.0",
            "@youwol/os-widgets": "^0.2.2",
            "d3": "^7.7.0",
            "codemirror": "^5.52.0",
            "@youwol/rx-code-mirror-editors": "^0.5.0",
            "@youwol/grapes-coding-playgrounds": "^0.2.0"
        },
        "aliases": []
    }
}
